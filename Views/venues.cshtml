@{
  Layout = "Shared/_Layout.cshtml";
}
      <h2>Venues</h2><br>

      @if(@Model.Count == 0)
      {
        <p>No venues have been added yet</p>
      }

      <ul>
        @foreach(var venue in Model)
        {
          <li><h3><a href='/venue/@venue.GetId()'>@venue.GetName()</a> | <a href="/venue/edit/@venue.GetId()">Edit</a> | <a href="/venue/delete/@venue.GetId()">Delete<a/></h3></li>
        }
      </ul><br>

      <h5><a href="/venues/new">Add new venue</a></h5>
      <h5><a href="/">Return home</a></h5><br>
